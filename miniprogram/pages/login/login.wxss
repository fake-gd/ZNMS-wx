/* 添加页面样式 */
input {
  display: block;
  width: 80%;
  margin: 10px auto;
  padding: 10px;
  border: 1px solid #ccc;
  border-radius: 4px;
}

button {
  display: block;
  width: 80%;
  margin: 10px auto;
  padding: 10px;
  background-color: #007aff;
  color: white;
  border: none;
  border-radius: 4px;
}

navigator {
  display: block;
  width: 80%;
  margin: 10px auto;
  text-align: center;
  color: #007aff;
}